# Interval between full cycles (seconds)
MTR_INTERVAL=300

# Number of packets per mtr run
MTR_COUNT=10

# Output directory inside the container (mount a host path here via -v)
MTR_OUTPUT_PATH=/data/mtr

# Comma-separated list of destinations
MTR_DESTINATIONS=custom-t0.speed.cloudflare.com,1.1.1.1,8.8.8.8
